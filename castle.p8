pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
function actors_i()
	local a={}
	a.ch="@"
	a.c=7
	add(actortypes,a)
	local a={}
	a.ch="t"
	a.c=3
	add(actortypes,a)
	local a={}
	a.ch="e"
	a.c=5
	add(actortypes,a)
end

function makeactor(t,ch,x,y,c)
	local a={}
	a.t=t
	a.ch=ch
	a.x=x
	a.y=y
	a.c=c
	a.shake=0
	add(actors,a)
	return a
end

function drawactor(a)
	print(a.ch,a.x*cellw+a.shake,a.y*cellh,a.c)
--	print(a.x,a.x*cellw+cellw,a.y*cellh,a.c)
--	print(a.y,a.x*cellw,a.y*cellh+cellh,a.c)
end

function shake(a)
	a.shake=sin(timer)
end

function doactor(a)
	local d=0
	if a.t==1 then
		d=btnp()
	end
	if a.t==3 then
		d=2^flr(rnd(4))
	end
	if d!=0 then
		local xdir=0 local ydir=0
		if d==1 then xdir=-1 end
		if d==2 then xdir= 1 end
		if d==4 then ydir=-1 end
		if d==8 then ydir= 1 end
		
		if room[a.x+xdir][a.y+ydir]==0 then
			room[a.x][a.y]=0
			a.x+=xdir a.y+=ydir
			room[a.x][a.y]=a.t
		end
	end
end

function loadmap(rx,ry,mx,my)
	for b=1,sector_s do
		for a=1,sector_s do
			room[a+rx*sector_s][b+ry*sector_s]=mget(a-1+mx*sector_s,b-1+my*sector_s)
		end
	end
end

function _init()
	timer=0
	cellw=5 cellh=6
	sector_s=16
	sector_a=4
	room_w=sector_s*sector_a
	level=0
	actortypes={}
	actors={}
	actors_i()
	room={}
	for a=1,room_w do
		room[a]={}
		for b=1,room_w do
			room[a][b]=0
		end
	end
	
	for b=0,sector_a-1 do
		for a=0,sector_a-1 do
			loadmap(a,b,flr(rnd(8)),level)
		end
	end
	for b=1,room_w do
		for a=1,room_w do
			local cell=room[a][b]
			if cell>0 then
				makeactor(cell,actortypes[cell].ch,a,b,actortypes[cell].c)
			end
		end
	end
	p=makeactor(1,actortypes[1].ch,5,5,actortypes[1].c)
end

function _draw()
	cls()
	foreach(actors,drawactor)
	rect(-2+cellw,-2+cellh,16*cellw+cellw-1,16*cellh+cellh)
--	print(xs,16*cellw+cellw,16*cellh+cellh)
end

function _update()
	if btnp()>0 then
		foreach(actors,doactor)
	end
	foreach(actors,shake)
	camera(p.x*cellw-10*cellw,p.y*cellh-8*cellh)
	timer+=1
end
